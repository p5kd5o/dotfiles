"\C-p":    previous-history
"\M-OA":   previous-history
"\M-[A":   previous-history
"-":       previous-history
"k":       previous-history
#          print-last-kbd-macro                          (not bound)
"\C-q":    quoted-insert
"\C-v":    quoted-insert
#          redraw-current-line                           (not bound)
